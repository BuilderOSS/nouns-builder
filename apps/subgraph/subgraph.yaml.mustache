specVersion: 1.3.0
description: Nouns Builder Subgraph
features:
  - nonFatalErrors
  - fullTextSearch
schema:
  file: ./schema.graphql
dataSources:
  - kind: ethereum
    name: Manager
    network: {{network}}
    source:
      abi: Manager
      address: '{{manager.address}}'
      startBlock: {{manager.startBlock}}
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - DAO
        - AuctionSettings
      abis:
        - name: Manager
          file: ./abis/Manager.json
        - name: Token
          file: ./abis/Token.json
        - name: MetadataRendererV1
          file: ./abis/MetadataRendererV1.json
        - name: MetadataRendererBase
          file: ./abis/MetadataRendererBase.json
        - name: Auction
          file: ./abis/Auction.json
      eventHandlers:
        - event: DAODeployed(address,address,address,address,address)
          handler: handleDAODeployed
        - event: MetadataRendererUpdated(address,address)
          handler: handleMetadataRendererUpdated
      file: ./src/manager.ts
{{#eas}}
  - kind: ethereum
    name: EAS
    network: {{network}}
    source:
      address: '{{eas.address}}'
      abi: EAS
      startBlock: {{eas.startBlock}}
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - DAO
        - Proposal
        - ProposalUpdate
      abis:
        - name: EAS
          file: ./abis/EAS.json
        - name: Token
          file: ./abis/Token.json
        - name: Governor
          file: ./abis/Governor.json
        - name: Treasury
          file: ./abis/Treasury.json
      eventHandlers:
        - event: Attested(indexed address,indexed address,bytes32,indexed bytes32)
          handler: handleAttested
          topic3:
            [
              '0x8bd0d42901ce3cd9898dbea6ae2fbf1e796ef0923e7cbb0a1cecac2e42d47cb3',
              '0x1289c5f988998891af7416d83820c40ba1c6f5ba31467f2e611172334dc53a0e',
              '0xc384fd4fdacb670667c07759423132a193053742b58d5a056b61d72ba1a09e26',
            ]
          calls:
            getAttestation: EAS[event.address].getAttestation(event.params.uid)
        - event: Revoked(indexed address,indexed address,bytes32,indexed bytes32)
          handler: handleRevoked
      file: ./src/eas.ts
{{/eas}}
{{#clanker}}
  - kind: ethereum
    name: Clanker
    network: {{network}}
    source:
      address: '{{clanker.address}}'
      abi: Clanker
      startBlock: {{clanker.startBlock}}
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - ClankerToken
        - DAO
      abis:
        - name: Clanker
          file: ./abis/Clanker.json
        - name: Treasury
          file: ./abis/Treasury.json
        - name: Governor
          file: ./abis/Governor.json
      eventHandlers:
        - event: TokenCreated(address,indexed address,indexed address,string,string,string,string,string,int24,address,bytes32,address,address,address,uint256,address[])
          handler: handleTokenCreated
      file: ./src/clanker.ts
{{/clanker}}
{{#zoraFactory}}
  - kind: ethereum
    name: ZoraFactory
    network: {{network}}
    source:
      address: '{{zoraFactory.address}}'
      abi: ZoraFactory
      startBlock: {{zoraFactory.startBlock}}
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - ZoraCoin
        - DAO
      abis:
        - name: ZoraFactory
          file: ./abis/ZoraFactory.json
        - name: Token
          file: ./abis/Token.json
      eventHandlers:
        - event: CoinCreatedV4(indexed address,indexed address,indexed address,address,string,string,string,address,(address,address,uint24,int24,address),bytes32,string)
          handler: handleCoinCreatedV4
      file: ./src/zoraFactory.ts
{{/zoraFactory}}
{{#zoraNFTCreator}}
  - kind: ethereum
    name: ZoraNFTCreator
    network: {{network}}
    source:
      address: '{{zoraNFTCreator.address}}'
      abi: ZoraNFTCreatorV1
      startBlock: {{zoraNFTCreator.startBlock}}
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - ZoraDrop
        - DAO
      abis:
        - name: ZoraNFTCreatorV1
          file: ./abis/ZoraNFTCreatorV1.json
        - name: ERC721Drop
          file: ./abis/ERC721Drop.json
        - name: EditionMetadataRenderer
          file: ./abis/EditionMetadataRenderer.json
        - name: Treasury
          file: ./abis/Treasury.json
        - name: Governor
          file: ./abis/Governor.json
      eventHandlers:
        - event: CreatedDrop(indexed address,indexed address,uint256)
          handler: handleCreatedDrop
      file: ./src/zoraNFTCreator.ts
{{/zoraNFTCreator}}
templates:
  - kind: ethereum
    name: Token
    network: {{network}}
    source:
      abi: Token
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - Token
        - DAO
        - DAOTokenOwner
      abis:
        - name: Token
          file: ./abis/Token.json
      eventHandlers:
        - event: Transfer(indexed address,indexed address,indexed uint256)
          handler: handleTransfer
        - event: DelegateChanged(indexed address,indexed address,indexed address)
          handler: handleDelegateChanged
      file: ./src/token.ts
{{#zoraNFTCreator}}
  - kind: ethereum
    name: ZoraDrop
    network: {{network}}
    source:
      abi: ERC721Drop
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - ZoraDrop
        - ZoraDropHolder
        - ZoraDropMintComment
      abis:
        - name: ERC721Drop
          file: ./abis/ERC721Drop.json
      eventHandlers:
        - event: Sale(indexed address,indexed uint256,indexed uint256,uint256)
          handler: handleSale
        - event: Transfer(indexed address,indexed address,indexed uint256)
          handler: handleTransfer
        - event: MintComment(indexed address,indexed address,indexed uint256,uint256,string)
          handler: handleMintComment
      file: ./src/zoraDropTemplate.ts
{{/zoraNFTCreator}}
  - kind: ethereum
    name: Governor
    network: {{network}}
    source:
      abi: Governor
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - Proposal
        - ProposalVote
      abis:
        - name: Governor
          file: ./abis/Governor.json
        - name: Treasury
          file: ./abis/Treasury.json
      eventHandlers:
        - event: ProposalCreated(bytes32,address[],uint256[],bytes[],string,bytes32,(address,uint32,uint32,uint32,uint32,uint32,uint32,uint32,uint32,bool,bool,bool))
          handler: handleProposalCreated
        - event: ProposalQueued(bytes32,uint256)
          handler: handleProposalQueued
        - event: ProposalExecuted(bytes32)
          handler: handleProposalExecuted
        - event: ProposalCanceled(bytes32)
          handler: handleProposalCanceled
        - event: ProposalVetoed(bytes32)
          handler: handleProposalVetoed
        - event: VoteCast(address,bytes32,uint256,uint256,string)
          handler: handleVoteCast
      file: ./src/governor.ts
  - kind: ethereum
    name: MetadataRendererV1
    network: {{network}}
    source:
      abi: MetadataRendererV1
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - DAO
        - MetadataItem
        - MetadataProperty
      abis:
        - name: MetadataRendererV1
          file: ./abis/MetadataRendererV1.json
        - name: MetadataRendererBase
          file: ./abis/MetadataRendererBase.json
        - name: Token
          file: ./abis/Token.json
      eventHandlers:
        - event: ContractImageUpdated(string,string)
          handler: handleContractImageUpdated
        - event: DescriptionUpdated(string,string)
          handler: handleDescriptionUpdated
        - event: WebsiteURIUpdated(string,string)
          handler: handleWebsiteURIUpdated
      {{!#callHandlers}}
        - event: PropertyAdded(uint256,string)
          handler: handlePropertyAdded
      {{!/callHandlers}}
      {{#callHandlers}}
      callHandlers:
        - function: addProperties(string[],(uint256,string,bool)[],(string,string))
          handler: handleAddProperties
        - function: deleteAndRecreateProperties(string[],(uint256,string,bool)[],(string,string))
          handler: handleDeleteAndRecreateProperties
      {{/callHandlers}}
      file: ./src/metadata.ts
  - kind: ethereum
    name: MetadataRendererBase
    network: {{network}}
    source:
      abi: MetadataRendererBase
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - DAO
        - MetadataItem
        - MetadataProperty
      abis:
        - name: MetadataRendererV1
          file: ./abis/MetadataRendererV1.json
        - name: MetadataRendererBase
          file: ./abis/MetadataRendererBase.json
        - name: Token
          file: ./abis/Token.json
      eventHandlers:
        - event: ContractImageUpdated(string,string)
          handler: handleContractImageUpdated
        - event: DescriptionUpdated(string,string)
          handler: handleDescriptionUpdated
        - event: WebsiteURIUpdated(string,string)
          handler: handleWebsiteURIUpdated
        - event: MetadataUpdate(uint256)
          handler: handleMetadataUpdate
        - event: BatchMetadataUpdate(uint256,uint256)
          handler: handleBatchMetadataUpdate
      file: ./src/metadata.ts
  - kind: ethereum
    name: Auction
    network: {{network}}
    source:
      abi: Auction
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - DAO
        - Auction
        - AuctionBid
        - AuctionConfig
      abis:
        - name: Auction
          file: ./abis/Auction.json
      eventHandlers:
        - event: AuctionCreated(uint256,uint256,uint256)
          handler: handleAuctionCreated
        - event: AuctionSettled(uint256,address,uint256)
          handler: handleAuctionSettled
        - event: AuctionBid(uint256,address,uint256,bool,uint256)
          handler: handleAuctionBid
        - event: DurationUpdated(uint256)
          handler: handleDurationUpdated
        - event: ReservePriceUpdated(uint256)
          handler: handleReservePriceUpdated
        - event: TimeBufferUpdated(uint256)
          handler: handleTimeBufferUpdated
        - event: MinBidIncrementPercentageUpdated(uint256)
          handler: handleMinBidIncrementPercentageUpdated
      file: ./src/auction.ts
{{#clanker}}
  - kind: ethereum
    name: ClankerToken
    network: {{network}}
    source:
      abi: ERC20
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - ClankerToken
        - ClankerTokenHolder
      abis:
        - name: ERC20
          file: ./abis/ERC20.json
      eventHandlers:
        - event: Transfer(indexed address,indexed address,uint256)
          handler: handleTransfer
      file: ./src/clankerTokenTemplate.ts
{{/clanker}}
{{#zoraFactory}}
  - kind: ethereum
    name: ZoraCoin
    network: {{network}}
    source:
      abi: ERC20
    mapping:
      kind: ethereum/events
      apiVersion: 0.0.7
      language: wasm/assemblyscript
      entities:
        - ZoraCoin
        - ZoraCoinHolder
      abis:
        - name: ERC20
          file: ./abis/ERC20.json
      eventHandlers:
        - event: Transfer(indexed address,indexed address,uint256)
          handler: handleTransfer
      file: ./src/zoraCoinTemplate.ts
{{/zoraFactory}}
