{
  "name": "@buildeross/hooks",
  "description": "Shared React hooks for BuilderOSS apps",
  "version": "0.2.1",
  "dependencies": {
    "@smartinvoicexyz/types": "^0.1.27",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@buildeross/constants": "workspace:*",
    "@buildeross/eslint-config-custom": "workspace:*",
    "@buildeross/ipfs-service": "workspace:*",
    "@buildeross/sdk": "workspace:*",
    "@buildeross/tsconfig": "workspace:*",
    "@buildeross/types": "workspace:*",
    "@buildeross/utils": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.21",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint": "8.57.1",
    "lodash": "^4.17.21",
    "swr": "^2.3.3",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "viem": "^2.44.2",
    "vitest": "^3.1.4",
    "wagmi": "^2.18.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "peerDependencies": {
    "@buildeross/constants": "0.2.1",
    "@buildeross/ipfs-service": "0.2.1",
    "@buildeross/sdk": "0.2.1",
    "@buildeross/types": "0.2.1",
    "@buildeross/utils": "0.2.1",
    "@types/lodash": "^4.17.17",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "viem": "^2.44.2",
    "wagmi": "^2.18.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BuilderOSS/nouns-builder.git",
    "directory": "packages/hooks"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist .turbo",
    "lint": "pnpm type-check && eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "types": "./dist/index.d.ts"
}
