{
  "name": "@zoralabs/zord",
  "version": "2.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "./dist/index.css"
  ],
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./index.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup",
    "build-icons": "svgr --out-dir src/icons --icon --replace-attr-values '#000=currentColor' --typescript icons ",
    "dev": "tsup --watch",
    "dev:types": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "lint": "eslint src --fix",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@vanilla-extract/css": "^1.17.2",
    "@vanilla-extract/private": "^1.0.3",
    "@vanilla-extract/recipes": "^0.5.5",
    "@vanilla-extract/sprinkles": "^1.5.0",
    "clsx": "^1.2.1",
    "react-polymorphic-types": "^2.0.0",
    "react-popper": "^2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@svgr/cli": "^6.5.1",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/uuid": "^9.0.0",
    "@vanilla-extract/esbuild-plugin": "^2.2.0",
    "tsconfig": "workspace:*",
    "eslint": "8.57.1",
    "eslint-config-custom": "workspace:*",
    "lodash-es": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^6.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
