{
  "name": "@buildeross/ipfs-service",
  "description": "IPFS client and utilities for BuilderOSS apps",
  "version": "0.2.0",
  "dependencies": {
    "sha.js": "^2.4.12"
  },
  "devDependencies": {
    "@buildeross/constants": "workspace:*",
    "@buildeross/eslint-config-custom": "workspace:*",
    "@buildeross/tsconfig": "workspace:*",
    "@types/sha.js": "^2.4.4",
    "eslint": "8.57.1",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "peerDependencies": {
    "@buildeross/constants": "0.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BuilderOSS/nouns-builder.git",
    "directory": "packages/ipfs-service"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist .turbo",
    "lint": "eslint src --fix",
    "test": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "types": "./dist/index.d.ts"
}
