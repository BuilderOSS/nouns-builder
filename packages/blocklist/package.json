{
  "name": "@buildeross/blocklist",
  "description": "OFAC sanctions blocklist utilities for BuilderOSS apps",
  "version": "0.2.1",
  "devDependencies": {
    "@buildeross/eslint-config-custom": "workspace:*",
    "@buildeross/tsconfig": "workspace:*",
    "@types/node": "^22.15.21",
    "@types/react": "^19.2.2",
    "eslint": "8.57.1",
    "tsup": "^8.3.5",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "peerDependencies": {
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "viem": "^2.38.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BuilderOSS/nouns-builder.git",
    "directory": "packages/blocklist"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist .turbo",
    "lint": "pnpm type-check && eslint src --fix",
    "type-check": "tsc --noEmit",
    "update": "./update_sdnlist.sh"
  },
  "types": "./dist/index.d.ts"
